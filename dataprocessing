  `source` varchar(45) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8$$


insert into CTPP_34.`A302103_tracts` select SUBSTRING(geoid,8,11),SUBSTRING(geoid,19,11),lineno, est,se,source from CTPP_34.A302103 where geoid like "C54%" and lineno = 8


