CREATE TABLE `A302103_tracts` (
  `from_tract` varchar(35) NOT NULL,
  `to_tract` varchar(35) NOT NULL,
  `lineno` int(22) DEFAULT NULL,
  `est` int(22) DEFAULT NULL,
